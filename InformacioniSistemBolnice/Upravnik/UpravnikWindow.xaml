<Window x:Class="InformacioniSistemBolnice.Upravnik.UpravnikWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InformacioniSistemBolnice.Upravnik"
        mc:Ignorable="d"
        Title="UpravnikWindow" Height="768" Width="1024">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Margin="10,0,0,10" Width="400" Height="718" VerticalAlignment="Bottom">
            <Button x:Name="button" Content="Prostorije" Background="CornflowerBlue" Height="40" Margin="50,450,0,10" FontSize="20" HorizontalAlignment="Left" Width="125" Click="WindowProstorije"/>
            <Button x:Name="button1" Content="Zaposleni" Background="CornflowerBlue" Height="40" Margin="50,10,0,10" FontSize="20" Width="125" HorizontalAlignment="Left"/>
            <Button x:Name="button2" Content="Lekovi" Background="CornflowerBlue" Height="40" FontSize="20" Margin="50,10,0,10" HorizontalAlignment="Left" Width="125" />
            <Button x:Name="button3" Content="Obaveštenja" Background="CornflowerBlue" Height="40" FontSize="20" Width="125" HorizontalAlignment="Left" Margin="50,10,0,10" />
        </StackPanel>
    </Grid>
</Window>
