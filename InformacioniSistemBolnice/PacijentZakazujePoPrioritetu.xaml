<Window x:Class="InformacioniSistemBolnice.PacijentZakazujePoPrioritetu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InformacioniSistemBolnice"
        mc:Ignorable="d"
        Title="Zakazivanje po prioritetu" Height="600" Width="600" WindowStartupLocation="CenterScreen" >
    <Grid>
        <StackPanel>
            <Label x:Name="label" Content="Izaberite prioritet petrage termina" HorizontalAlignment="Left" Margin="136,36,0,0" VerticalAlignment="Top" FontSize="22" Height="40"/>
            <RadioButton x:Name="doctorRadioButton" Content="Lekar" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="225,15,0,0" FontSize="19" Checked="doctorRadioButton_Checked" RenderTransformOrigin="0.312,-2.333" />
            <RadioButton x:Name="timeRadioButton" Content="Vremenski interval" HorizontalAlignment="Left" Margin="225,15,0,0" VerticalAlignment="Top" FontSize="19" Checked="timeRadioButton_Checked" />
        </StackPanel>
        <Frame x:Name="Main" NavigationUIVisibility="Hidden" Margin="10,170,10,10" ></Frame>
        
    </Grid>
</Window>
