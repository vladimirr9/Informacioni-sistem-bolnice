<Window x:Class="InformacioniSistemBolnice.Patient_ns.StartPatientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InformacioniSistemBolnice.Patient_ns"
        mc:Ignorable="d"
        Title="StartPatientWindow" Height="550" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="odjava"  FontSize="18" Background="White" BorderThickness="2" BorderBrush="Black" Content="Odjava" HorizontalAlignment="Left" Margin="697,23,0,0" VerticalAlignment="Top" Width="75" Click="odjava_Click" />
        <TextBlock x:Name="imePacijenta" FontStyle="Italic" HorizontalAlignment="Left" Margin="22,23,0,0" TextWrapping="Wrap" Text="" FontSize="25" VerticalAlignment="Top"/>
        <Label x:Name="imeLjekara" FontStyle="Italic" HorizontalAlignment="Left" Margin="22,23,0,0"  FontSize="25" VerticalAlignment="Top"/>

        <Frame x:Name="startWindow" NavigationUIVisibility="Hidden" Margin="0,85,0,0" />
        <Label x:Name="titleLabel"  Visibility="Hidden"  VerticalAlignment="Top" Margin="270,21,100,0" Height="45" FontSize="25" />
        <Label x:Name="titlePriorityLabel"  Visibility="Hidden"  VerticalAlignment="Top" Margin="230,21,100,0" Height="45" FontSize="25" />
    </Grid>
</Window>
